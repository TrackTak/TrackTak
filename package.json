{
  "name": "@tracktak/monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/**"
  ],
  "engines": {
    "yarn": "^1.17.3",
    "node": "^16.3.0"
  },
  "scripts": {
    "generate-exh-pairs": "node --es-module-specifier-resolution=node ./scripts/writeECBExchangePairsToJSON.js",
    "generate-fundamentals": "node --es-module-specifier-resolution=node ./scripts/writeBulkFundamentalsDataToJSON.js",
    "find-missing-gbonds": "node --es-module-specifier-resolution=node ./scripts/findMissingGBonds",
    "clean": "lerna clean",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"packages/**/*.{jsx,js}\" --write",
    "fix:lint": "eslint packages --ext .{jsx,js} --fix",
    "release": "lerna version",
    "prettier:windows": "./node_modules/.bin/prettier --write \"{packages}/**/*.{jsx,js}\""
  },
  "devDependencies": {
    "@octokit/core": "^3.5.1",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "editorconfig": "^0.15.3",
    "npm-run-all": "^4.1.5",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "lerna": "^3.22.1",
    "prettier": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "3.9.10"
  }
}
