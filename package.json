{
  "name": "tracktak",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "node": "npm run start --prefix ./node",
    "ui": "npm run start --prefix ./ui",
    "start": "run-p node ui",
    "clean": "rimraf ./node_modules",
    "install:ui": "cd ./ui && npm install",
    "install:node": "cd ./node && npm install",
    "install": "run-p install:ui install:node"
  },
  "devDependencies": {
    "editorconfig": "^0.15.3",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
