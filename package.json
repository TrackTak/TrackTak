{
  "name": "@tracktak/monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/**"
  ],
  "engines": {
    "yarn": "^1.17.3",
    "node": ">=12.13.0"
  },
  "scripts": {
    "generate-exh-pairs": "node --es-module-specifier-resolution=node ./scripts/writeECBExchangePairsToJSON.js",
    "generate-fundamentals": "node --es-module-specifier-resolution=node ./scripts/writeBulkFundamentalsDataToJSON.js",
    "find-missing-gbonds": "node --es-module-specifier-resolution=node ./scripts/findMissingGBonds",
    "clean": "lerna clean",
    "release": "lerna version",
    "prettier:windows": "./node_modules/.bin/prettier --write \"{packages}/**/*.{jsx,js}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "editorconfig": "^0.15.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "lerna": "^3.22.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
