{
  "name": "tracktak-node",
  "version": "0.1.0",
  "main": "./app.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": "12.20.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./app.js --es-module-specifier-resolution=node",
    "start:dev": "cross-env NODE_ENV=development nodemon --es-module-specifier-resolution=node",
    "zip": "cd ./build && bestzip ../build.zip *",
    "build": "webpack && npm run zip",
    "clean": "rimraf ./node_modules",
    "deploy": "npm run build && eb deploy"
  },
  "devDependencies": {
    "bestzip": "^2.1.7",
    "copy-webpack-plugin": "^6.3.1",
    "dotenv-flow": "^3.2.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2",
    "worker-plugin": "^5.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.0",
    "comlink": "^4.3.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "iso-3166-1-alpha-2": "^1.0.0",
    "memory-cache": "^0.2.0"
  }
}
