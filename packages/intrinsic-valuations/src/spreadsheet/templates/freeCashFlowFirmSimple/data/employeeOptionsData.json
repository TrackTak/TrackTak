    },
    "9": {
      "0": "d1",
      "1": "=IFERROR((LN(B8 / B3) + (B6 + (B5 * B5) / 2) * B4) / (B5 * SQRT(B4)), 0)"
    },
    "10": {
      "0": "N (d1)",
      "1": "=NORMDIST(B10, 0, 1, TRUE)"
    },
    "12": {
      "0": "d2",
      "1": "=B10 - B5 * SQRT(B4)"
    },
    "13": {
      "0": "N (d2)",
      "1": "=NORMDIST(B13, 0, 1, TRUE)"
    },
    "15": {
      "0": "Value per option",
      "1": "=B8 * B11 - B3 * EXP(-B6 * B4) * B14"
    },
    "16": {
      "0": "Value of all options",
      "1": "=B16*B2"
    }
  }
