{
  "name": "@tracktak/intrinsic-valuations",
  "version": "1.1.1",
  "description": "A react package that allows external websites to use our intrinsic valuations for https://tracktak.com",
  "author": "tracktak",
  "license": "SEE LICENSE IN README.MD",
  "type": "module",
  "repository": "git://github.com/trackTak/tracktak-example.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.module.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=12.20.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "copy-assets": "copyfiles -f \"../web-spreadsheet/src/assets/**.svg\" ./dist/assets",
    "build": "cross-env NODE_ENV=production microbundle --no-sourcemap && npm run copy-assets",
    "build:dev": "rimraf ./dist && cross-env NODE_ENV=production microbundle --compress false && npm run copy-assets",
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./node_modules",
    "analyze-size": "rimraf ./dist && cross-env NODE_ENV=production microbundle -f modern && source-map-explorer ./dist/index.modern.module.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@material-ui/core": "5.0.0-alpha.25",
    "@material-ui/icons": "5.0.0-alpha.24",
    "@material-ui/styles": "5.0.0-alpha.25",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.21.1",
    "babel-plugin-file-loader": "^2.0.0",
    "eventemitter3": "^4.0.7",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "change-case": "^4.1.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.2",
    "currency-symbol-map": "^4.0.4",
    "dayjs": "^1.9.7",
    "iso-3166-1-alpha-2": "^1.0.0",
    "less": "^4.1.1",
    "lodash-es": "^4.17.20",
    "microbundle": "^0.13.0",
    "postcss": "^8.2.15",
    "query-string": "^7.0.0",
    "react-number-format": "^4.5.0",
    "react-redux": "^7.2.2",
    "react-show-more": "^2.0.0",
    "hyperformula": "^0.6.0",
    "react-table": "^7.6.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "xlsx": "^0.16.9"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@fontsource/nunito": "^4.2.2",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2"
  },
  "optionalPeerDependencies": {
    "gatsby": "^2.32.8",
    "gatsby-plugin-anchor-links": "^1.2.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "discounted cash flow",
    "dcf",
    "react",
    "tracktak",
    "cash flow"
  ]
}
