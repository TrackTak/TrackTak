{
  "name": "@tracktak/dcf-react",
  "version": "3.0.0-beta.2",
  "description": "A react package that allows external websites to use our discounted cash flow calculator for https://tracktak.com",
  "author": "tracktak",
  "license": "CC BY-ND 4.0",
  "repository": "git://github.com/trackTak/tracktak-example.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.module.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=12.20.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/table": "3.7.1"
  },
  "scripts": {
    "build": "rimraf ./dist && cross-env NODE_ENV=production microbundle -f modern,es,cjs --worker-loader --sourcemap false",
    "build:dev": "rimraf ./dist && cross-env NODE_ENV=production microbundle -f modern,es,cjs --worker-loader --compress false",
    "prepublish": "npm run build",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean": "rimraf ./node_modules",
    "analyze-size": "rimraf ./dist && cross-env NODE_ENV=production microbundle -f modern --worker-loader && source-map-explorer ./dist/index.modern.module.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/table": "3.7.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@formulajs/formulajs": "^2.6.2",
    "@material-ui/core": "5.0.0-alpha.25",
    "@material-ui/icons": "5.0.0-alpha.24",
    "@material-ui/styles": "5.0.0-alpha.25",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.4.0",
    "change-case": "^4.1.2",
    "currency-symbol-map": "^4.0.4",
    "dayjs": "^1.9.7",
    "iso-3166-1-alpha-2": "^1.0.0",
    "lodash": "^4.17.20",
    "mathjs": "^8.0.1",
    "microbundle": "github:timdaub/microbundle#feat/syntax-worker-detection",
    "react-dom": "^17.0.1",
    "react-number-format": "^4.5.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-show-more": "^2.0.0",
    "react-table": "^7.6.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-in-worker": "^0.7.0",
    "styled-components": "^5.2.1",
    "worker-plugin": "^5.0.0",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@fontsource/nunito": "^4.2.2",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.17",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cross-env": "^7.0.2",
    "dotenv-flow": "^3.2.0",
    "node-sass": "^5.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "source-map-explorer": "^2.5.2"
  },
  "optionalPeerDependencies": {
    "gatsby": "^2.32.8",
    "gatsby-plugin-anchor-links": "^1.2.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "discounted cash flow",
    "dcf",
    "react",
    "tracktak",
    "cash flow"
  ]
}
