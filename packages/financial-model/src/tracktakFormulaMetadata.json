  "DATA_ANALYSIS.SENSITIVITY_ANALYSIS": {
    "header": "DATA_ANALYSIS.SENSITIVITY_ANALYSIS",
    "headerDescription": "Determines how different values of an independent variable affect a particular dependent variable under a given set of assumptions.",
    "type": "Data Analysis",
    "parameters": [
      "intersection_point",
      "x_var",
      "y_var",
      "xMin",
      "xMax",
      "yMin",
      "yMax"
    ],
    "codeSyntaxUsage": [
      "=DATA_ANALYSIS.SENSITIVITY_ANALYSIS(B1,B2,B3,15,20,3,30)"
    ],
    "codeSyntaxElements": [
      {
        "codeSyntax": "DATA_ANALYSIS.SENSITIVITY_ANALYSIS(intersection_point, x_var, y_var, xMin, xMax, yMin, yMax)",
        "values": [
          {
            "syntaxName": "intersection_point",
            "description": "The outcome value. This must be a cell reference and not a scalar value. We can determine how changes in variables affect the outcome value (intersection_point), for example the estimated value of a stock."
          },
          {
            "syntaxName": "x_var",
            "description": "1st input variable. This must be a cell reference and not a scalar value. Example: Cost of Capital (WACC) cell."
          },
          {
            "syntaxName": "y_var",
            "description": "2nd input variable. This must be a cell reference and not a scalar value. Example: Growth Rates cell."
          },
          {
            "syntaxName": "xMin",
            "description": "Min of the 1st input variable. Example: min of WACC is 2%."
          },
          {
            "syntaxName": "xMax",
            "description": "Max of the 1st input variable. Example: max of WACC is 10%."
          },
          {
            "syntaxName": "yMin",
            "description": "Min of the 2nd input variable. Example: min of growth rate is 1%."
          },
          {
            "syntaxName": "yMax",
            "description": "Max of the 2nd input variable. Example: max of growth rate is 6%."
          }
        ]
      }
    ],
    "attributes": []
  },
  "DATA_ANALYSIS.MONTE_CARLO_SIMULATION": {
    "header": "DATA_ANALYSIS.MONTE_CARLO_SIMULATION",
    "headerDescription": "Is a model used to predict the probability of different outcomes when the intervention of randomness is present.",
    "type": "Data Analysis",
    "parameters": ["intersection_point", "{cell_references}", "{var_assumptions}", "[iteration]"],
    "codeSyntaxUsage": [
      "=DATA_ANALYSIS.MONTE_CARLO_SIMULATION(B3,{B1,B2})",
      "=DATA_ANALYSIS.MONTE_CARLO_SIMULATION(A3,{A1},100)"
    ],
    "codeSyntaxElements": [
      {
        "codeSyntax": "DATA_ANALYSIS.MONTE_CARLO_SIMULATION(intersection_point, {cell_references}, {var_assumptions}, [iteration])",
        "values": [
          {
            "syntaxName": "intersection_point",
            "description": "The point of intersection (forecast value) of the assumptions that links the desired output."
          },
          {
            "syntaxName": "{cell_references}",
            "description": "Array of cell references."
          },
          {
            "syntaxName": "{var_assumptions}",
            "description": "Array of assumptions."
          },
          {
            "syntaxName": "[iteration]",
            "description": "[OPTIONAL]. Sets the number of trials to run."
          }
        ]
      }
    ],
    "attributes": []
  },
