      {
        "codeSyntax": "STATISTICS.UNIFORM_INVERSE_RANDOM(min, max)",
        "values": [
          {
            "syntaxName": "min",
            "description": "Minimum fixed value."
          },
          {
            "syntaxName": "max",
            "description": "Maximum fixed value."
          }
        ]
      }
    ],
    "attributes": []
  },
  "STATISTICS.TRIANGULAR_INVERSE_RANDOM": {
    "header": "STATISTICS.TRIANGULAR_INVERSE_RANDOM",
    "headerDescription": "Defines a minimum, maximum and most likely value, also referred to as peak value. It is useful with limited data in situations such as sales performance, work hours, inventory numbers, and marketing costs.",
    "type": "Probability Distribution",
    "parameters": ["min", "most_likely", "max"],
    "codeSyntaxUsage": [
      "=STATISTICS.TRIANGULAR_INVERSE_RANDOM(A1,A2,A3)",
