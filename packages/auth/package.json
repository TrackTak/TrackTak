{
  "name": "@tracktak/auth",
  "version": "1.0.0",
  "description": "A package for handling authentication for https://tracktak.com",
  "author": "tracktak",
  "license": "CC BY-ND 4.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.modern.module.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=12.20.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production microbundle --no-sourcemap",
    "build:dev": "rimraf ./dist && cross-env NODE_ENV=production microbundle --compress false",
    "prepublishOnly": "npm run build",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean": "rimraf ./node_modules",
    "analyze-size": "rimraf ./dist && cross-env NODE_ENV=production microbundle -f modern && source-map-explorer ./dist/index.modern.module.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@reduxjs/toolkit": "^1.4.0",
    "amazon-cognito-identity-js": "^4.6.0",
    "axios": "^0.21.1",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.2",
    "currency-symbol-map": "^4.0.4",
    "dayjs": "^1.9.7",
    "iso-3166-1-alpha-2": "^1.0.0",
    "lodash": "^4.17.20",
    "microbundle": "^0.13.0",
    "node-sass": "^5.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss": "^8.2.10",
    "query-string": "^7.0.0",
    "react-number-format": "^4.5.0",
    "react-redux": "^7.2.2",
    "react-show-more": "^2.0.0",
    "react-table": "^7.6.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "sass-loader": "^11.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@fontsource/nunito": "^4.2.2",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2"
  },
  "optionalPeerDependencies": {
    "gatsby": "^2.32.8",
    "gatsby-plugin-anchor-links": "^1.2.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "discounted cash flow",
    "dcf",
    "react",
    "tracktak",
    "cash flow",
    "auth"
  ]
}
